<aura:component implements="flexipage:availableForRecordHome,force:hasRecordId" controller="ContactPhoneListController" access="global" >
    <aura:attribute name="Contacts" type="Contact" />
    <aura:attribute name="Columns" type="List" />

    <aura:handler name="init" value="{!this}" action="{!c.myAction}" />

    <lightning:datatable data="{!v.Contacts }" columns="{! v.Columns }" keyField="Id" hideCheckboxColumn="true"/>

</aura:component>

